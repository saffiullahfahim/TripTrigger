{
  "name": "Trip Trigger",
  "version": "1.0.0",
  "description": "Web Automation for Auto Populating Fields on Travel Booking Website.",
  "manifest_version": 3,
  "host_permissions": [
    "https://www.gotogate.co.uk/*",
    "https://www.gotogate.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.gotogate.co.uk/*",
        "https://www.gotogate.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
